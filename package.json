{
  "name": "@ampersarnie/implements",
  "version": "0.2.0",
  "description": "Add Traits and Interfaces to your javascript classes.",
  "main": "./implements.js",
  "options": {},
  "scripts": {
    "mocha": "node_modules/.bin/mocha --growl test/tests.js",
    "mocha:report": "node_modules/.bin/mocha --reporter markdown test/tests.js > test/RESULTS.md; echo $(($(date +'%s * 1000 + %-N / 1000000'))) >> test/RESULTS.md",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:test": "npm run build; npm run mocha",
    "prepublish": "npm run build; npm run mocha; npm run mocha:report",
    "lint": "eslint src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ampjs/Implements.git"
  },
  "bugs": {
    "url": "https://github.com/ampjs/Implements/issues"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "react",
    "javascript",
    "traits",
    "interface",
    "es6"
  ],
  "author": "Paul Taylor <hello@ampersandwich.co.uk> (http://ampersandwich.co.uk)",
  "maintainers": [
    {
      "email": "hello@ampersandwich.co.uk",
      "name": "ampersarnie"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {}
}
